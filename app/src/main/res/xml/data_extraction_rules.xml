<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <include domain="database" path="miner_db"/>
        <include domain="database" path="miner_db-wal"/>
        <include domain="database" path="miner_db-shm"/>
        <include domain="file" path="datastore/miner_preferences.preferences_pb"/>
        <include domain="file" path="datastore/achievement_store.preferences_pb"/>
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>